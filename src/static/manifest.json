{
  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon128.png",
    "default_title": "Yaba - Reminders, bookmarks, workspaces and more"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "description": "Open extension",
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E"
      }
    },
    "bookmark-tab": {
      "description": "Bookmark the current tab",
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    },
    "toggle-notes": {
      "description": "Toggle notes visibility",
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "content_scripts": [
    {
      "js": ["contentScript.js", "foreground.js"],
      "match_origin_as_fallback": true,
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "description": "Set reminders, bookmark your favorite pages, collaborate through your workspaces, and more.",
  "host_permissions": [
    "\u003Call_urls>",
    "http://*/",
    "https://*/",
    "https://www.googleapis.com/",
    "https://oauth2.googleapis.com/"
  ],
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "key": "",
  "manifest_version": 3,
  "name": "Yaba - Reminders, bookmarks, workspaces and more",
  "oauth2": {
    "client_id": "410458758559-t4paj8767ftlpi12pa4i91m5rb42n1gm.apps.googleusercontent.com",
    "scopes": [
      "openid",
      "https://www.googleapis.com/auth/contacts",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/contacts.readonly",
      "https://www.googleapis.com/auth/profile.emails.read",
      "https://www.googleapis.com/auth/contacts.other.readonly"
    ]
  },
  "permissions": [
    "tabs",
    "alarms",
    "storage",
    "history",
    "identity",
    "bookmarks",
    "scripting",
    "activeTab",
    "unlimitedStorage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.4",
  "web_accessible_resources": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "resources": [
        "gsi-client.js",
        "images/Lamp.svg",
        "images/Torso.svg",
        "Satoshi-Variable.ttf",
        "SuisseIntl-Book.woff",
        "styles/foreground.css",
        "images/placeholder.png",
        "NeueMontreal-Medium.woff2"
      ]
    }
  ]
}
